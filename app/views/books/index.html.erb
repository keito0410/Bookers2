<%= render 'users/list', book: @book, user: current_user %>
      <div class="col-xs-9">
        <h1>Users</h1>
          <table class="table">
          <thead>
          <tr>
          <th>image</th>
          <th>title</th>
          <th>body</th>
          </tr>
          </thead>
          <tbody>
          <% @books.each do |book| %>
          <tr>
          <td><%= link_to (user_path(book.user.id)) do %>
            <%= attachment_image_tag book.user, :profile_image, size: "100x100" %>
            <% end %>
          </td>
          <td>
            <%= link_to book_path(book.id) do %>
            <%= book.title %>
              <% end %>
            </td>
          <td><%= book.body %></td>
          <td><%= link_to 'Show', book %></td>
        </tr>
      <% end %>
    </tbody>
          </table>
      </div>
    </div>
  </div>
</div>